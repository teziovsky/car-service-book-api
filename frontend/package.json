{
  "name": "car-service-book-app",
  "version": "0.0.1",
  "description": "React 18 Frontend for car-service-book project",
  "homepage": "https://github.com/teziovsky/car-service-book/#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:teziovsky/car-service-book.git"
  },
  "engines": {
    "node": ">=v18.0.0",
    "npm": ">=v8.0.0"
  },
  "bugs": {
    "url": "https://github.com/teziovsky/car-service-book/issues",
    "email": "jakub.soboczynski@icloud.com"
  },
  "license": "MIT License",
  "author": {
    "name": "teziovsky",
    "email": "jakub.soboczynski@icloud.com",
    "url": "https://www.jakubsoboczynski.pl"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix --quiet",
    "format": "prettier --check '**/*.{js,jsx,ts,tsx,css,scss,md,json}'",
    "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx,css,scss,md,json}'",
    "type-check": "tsc",
    "prepare": "cd .. && husky install frontend/.husky",
    "clean": "rimraf dist node_modules package-lock.json"
  },
  "lint-staged": {
    "*.{jsx,js,ts,tsx,scss,json,md}": "prettier --cache --write",
    "*.{jsx,js,ts,tsx}": "eslint --cache --fix",
    "*.styled.ts": "stylelint --fix"
  },
  "dependencies": {
    "axios": "0.27.2",
    "focus-trap-react": "10.0.0",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-cookie": "4.1.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-react": "^2.0.1",
    "autoprefixer": "10.4.11",
    "babel-loader": "^8.2.5",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "8.4.16",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "rimraf": "3.0.2",
    "stylelint": "^14.11.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "tailwindcss": "3.1.8",
    "typescript": "^4.8.2",
    "vite": "^3.0.9"
  }
}
