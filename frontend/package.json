{
  "name": "car-service-book-app",
  "version": "0.0.1",
  "description": "Car service book app made with ❤️ with Next.js",
  "homepage": "https://github.com/teziovsky/car-service-book/#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:teziovsky/car-service-book.git"
  },
  "engines": {
    "node": "16.x",
    "npm": ">=v7.0.0"
  },
  "bugs": {
    "url": "https://github.com/teziovsky/car-service-book/issues",
    "email": "teziovsky@gmail.com"
  },
  "license": "The Unlicense",
  "author": {
    "name": "teziovsky",
    "email": "teziovsky@gmail.com",
    "url": "https://www.jakubsoboczynski.pl"
  },
  "private": true,
  "scripts": {
    "clean": "rimraf dist node_modules pnpm-lock.yaml yarn.lock package-lock.json",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "eslint": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,scss,md,json}' --loglevel=warn",
    "lint": "npm run prettier && npm run eslint",
    "typecheck": "tsc --noEmit",
    "prepare": "cd ../ && husky install ./frontend/.husky"
  },
  "lint-staged": {
    "*.{jsx,js,ts,tsx,scss,json,md}": "prettier --cache --write",
    "*.{jsx,js,ts,tsx}": "eslint --cache --fix"
  },
  "dependencies": {
    "@tanstack/react-query": "4.13.0",
    "axios": "^1.1.3",
    "classnames": "2.3.2",
    "daisyui": "2.33.0",
    "focus-trap-react": "^10.0.0",
    "next": "13.0.0",
    "react": "18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "18.2.0",
    "react-icons": "^4.6.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.7",
    "@tanstack/react-query-devtools": "4.13.0",
    "@types/node": "18.11.4",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "10.4.12",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "husky": ">=7",
    "lint-staged": ">=10",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "tailwindcss": "3.2.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
