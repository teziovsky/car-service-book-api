{
  "name": "car-service-book-app",
  "version": "0.0.1",
  "description": "React 18 Frontend for car-service-book project",
  "homepage": "https://github.com/teziovsky/car-service-book/#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:teziovsky/car-service-book.git"
  },
  "engines": {
    "node": ">=v18.0.0",
    "npm": ">=v8.0.0"
  },
  "bugs": {
    "url": "https://github.com/teziovsky/car-service-book/issues",
    "email": "jakub.soboczynski@icloud.com"
  },
  "license": "MIT License",
  "author": {
    "name": "teziovsky",
    "email": "jakub.soboczynski@icloud.com",
    "url": "https://www.jakubsoboczynski.pl"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix --quiet",
    "format": "prettier --check '**/*.{js,jsx,ts,tsx,css,scss,md,json}'",
    "format:fix": "prettier --write '**/*.{js,jsx,ts,tsx,css,scss,md,json}'",
    "type-check": "tsc",
    "prepare": "cd .. && husky install frontend/.husky",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{jsx,js,ts,tsx,scss,json,md}": "prettier --cache --write",
    "*.{jsx,js,ts,tsx}": "eslint --cache --fix"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "^18.7.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-react": "^2.0.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  }
}
